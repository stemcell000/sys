<div class="row">
	<h2>Users</h2>
</div>
<%unless @users.empty?%>
<div class="row">
	<div class="col-lg-12 col-md-12 col-sm-12">
		<%= render 'users/components/search' %>
	</div>
</div>
<div class="row">
	<div class="col-xs-12">
				<%= render partial: "users/list_components/list", locals: {users: @users}%>
		<%else%>
				<p>No user yet.</p>
		<%end%>
	</div>
</div>