<h5>Boxes</h5>
<small>
  <div class="container-fluid">
	  <table class="table table-striped">
	    <thead>
	      
	    <th class="col-lg-5 col-md-5 col-sm-5">
	        Name
	    </th>
	    	 <th class="col-lg-4 col-md-4 col-sm-4">
	        Barcode
	    </th>
	    <th class="col-lg-3 col-md-3 col-sm-3 center"></th>
	    
	    </thead>
	    <tbody>
	    	 <% unless smart_listing.empty? %>
		  	<% smart_listing.collection.to_a.uniq.each do |s| %>
	      		<tr class="editable" data-id = "<%= s.id %>">
	          		<%= smart_listing.render object: s, partial: "boxes/smart_listing/box", locals: {object: s} %>
	        		</tr>
	      	<% end %>
	      <%else%>
	      	No box
	     <%end%>
	    </tbody>
	  </table>
	  <%= smart_listing.paginate %>
	  <%= smart_listing.pagination_per_page_links %>
	</div>
</small>