<%if box.rack_position%>
        <div class="text-left container_rack_label_small">
          rack#<%=box.rack_position.shelf_rack.name%>
        </div>
    <div class="container_rack_small">
            <%box.rack_position.shelf_rack.rack_positions.each do |position|%>

                <% bg_color = box.rack_position.id == position.id ? '#5cb85c':'#337ab7' %>
                <div class = "col-xs-11 bg-default container_rack_position_small"style="display: inline-block; height: 100%; margin-top: 5px; margin-bottom: 2px; padding: 0 px; background-color: <%=bg_color%>;">
                </div>
                <div class="col-xs-1" style="font-size: 50%; padding: 0 0 0 0;"><%=position.name%>
                </div>
            <%end%>
    </div>
<%end%>