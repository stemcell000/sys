<div class="container_rack">
    <div class="text-left container_rack_label">
      rack#<%=shelf_rack.name%>
    </div>
        <%shelf_rack.rack_positions.each do |position|%>
            <div class = "col-xs-11  bg-default container_rack_position draggableContainer slot" data-id=<%=position.id%> data-name=<%=position.name%> >
