		<div class="col-xs-12 text-center" style="overflow: auto;">
			<%= render partial: "batches/list_components/list", locals: {batches: @batches} %>
				</div>
		</div>
	</div>
<%else%>
<p><em>No batches</em></p>
<%end%>

