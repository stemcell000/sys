<%if container.container_type_id== 2%>
      <div class="row">
        <div class="col-xs-3" style="overflow: auto;">
          <%=render partial: 'containers/components/top_view', locals: {container: container}%>
        </div>
      <div class="col-xs-6" style="overflow: auto;">
        <%=render partial: 'containers/components/side_view', locals: {container: container}%>
      </div>
<%else%>
    <div class="row">
    <% container.shelves.each do |shelf| %>
        <%=render partial: 'containers/components/front_view', locals: {container: container, shelf: shelf}%>
      <%end%>
    </div>
  <%end%>