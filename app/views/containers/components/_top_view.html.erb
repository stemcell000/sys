  <h5>Top view</h5>
  <% container.shelves.each do |shelf| %>
    <div style="display: inline-block; text-align: middle; margin: 0 0 0 0; ">
      <center><strong>col. <%=shelf.name%></strong></center>
         <% shelf.shelf_racks.each do |rack| %>
                <div style="  min-width: 70px; min-height: 70px; max-width: 70px; max-height: 70px; border: 1px solid black; border-radius: 2px; padding: 0 0 0 7% ; margin: 0 0 5% 0 ; background-color: #337ab7; font-size: 90%; text-align: center; color: white;">
                    rack#<%=rack.name%>
                </div>
          <%end%>
    </div>
  <%end%>